91891571098839ed1cf20993ad2cbeb1c/aRez-0.1.5-py3-none-any.whl"}, {"comment_text": "", "digests": {"md5": "01bb8e5f2bc9729f6620d5c86dccf1cd", "sha256": "62dbc71821d07476f68ba89bae657d78b62b05822e5640b264c62e9541ed9520"}, "downloads": -1, "filename": "aRez-0.1.5.tar.gz", "has_sig": false, "md5_digest": "01bb8e5f2bc9729f6620d5c86dccf1cd", "packagetype": "sdist", "python_version": "source", "requires_python": ">=3.8", "size": 38244, "upload_time": "2020-04-13T15:20:42", "upload_time_iso_8601": "2020-04-13T15:20:42.198279Z", "url": "https://files.pythonhosted.org/packages/de/bb/cb2a5985e393757cc37e38204bcdbf1515b96fcbee3e92e6fc2a421cc385/aRez-0.1.5.tar.gz"}]}, "urls": [{"comment_text": "", "digests": {"md5": "aff823e560290f8159fc8a20832276d5", "sha256": "50d9e552c6f9b8ebe0652274d6d638e008ffa6c9e342a69219739893d10a595c"}, "downloads": -1, "filename": "aRez-0.1.5-py3-none-any.whl", "has_sig": false, "md5_digest": "aff823e560290f8159fc8a20832276d5", "packagetype": "bdist_wheel", "python_version": "py3", "requires_python": ">=3.8", "size": 58957, "upload_time": "2020-04-13T15:20:38", "upload_time_iso_8601": "2020-04-13T15:20:38.468327Z", "url": "https://files.pythonhosted.org/packages/10/e3/8320d4bc3977ce18bece505484191891571098839ed1cf20993ad2cbeb1c/aRez-0.1.5-py3-none-any.whl"}, {"comment_text": "", "digests": {"md5": "01bb8e5f2bc9729f6620d5c86dccf1cd", "sha256": "62dbc71821d07476f68ba89bae657d78b62b05822e5640b264c62e9541ed9520"}, "downloads": -1, "filename": "aRez-0.1.5.tar.gz", "has_sig": false, "md5_digest": "01bb8e5f2bc9729f6620d5c86dccf1cd", "packagetype": "sdist", "python_version": "source", "requires_python": ">=3.8", "size": 38244, "upload_time": "2020-04-13T15:20:42", "upload_time_iso_8601": "2020-04-13T15:20:42.198279Z", "url": "https://files.pythonhosted.org/packages/de/bb/cb2a5985e393757cc37e38204bcdbf1515b96fcbee3e92e6fc2a421cc385/aRez-0.1.5.tar.gz"}]}